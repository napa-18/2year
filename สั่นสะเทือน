int sensor = D3;
int led1 = D10;

void setup() {
 Serial.begin(9600);
 pinMode(sensor,INPUT);
 pinMode(led1,OUTPUT); 
}

void loop() {
  uint32_t ON,OFF;
  ON=pulseIn(sensor,HIGH);
  OFF=pulseIn(sensor,LOW);
  int period=ON+OFF;
  float frequency = pow(10,6)/period;
  if(frequency>100){
    Serial.println(frequency);
    digitalWrite(led1,HIGH);
    delay(500);
  }else{
    frequency=0;
    Serial.println(frequency);
    digitalWrite(led1,LOW);
    
  }
}
